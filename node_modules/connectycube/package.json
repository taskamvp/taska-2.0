{
  "name": "connectycube",
  "description": "ConnectyCube chat and video chat JavaScript SDK",
  "version": "4.7.0",
  "homepage": "https://developers.connectycube.com/js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/connectycube.min.js",
  "browser": "./dist/connectycube.min.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.esm.js",
      "require": "./dist/types.cjs",
      "types": "./dist/types/types/index.d.ts"
    },
    "./umd": {
      "default": "./dist/connectycube.min.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "license": "Apache-2.0",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean & rollup -c",
    "dev": "npm run clean & rollup -c -w",
    "test:browser": "jasmine-browser-runner serve --config=__tests__/support/jasmine-browser.json",
    "test:node": "jasmine --config=__tests__/support/jasmine-node.json",
    "coverage:node": "nyc --reporter=lcov npm run test:node && open-cli ./coverage/lcov-report/index.html"
  },
  "bin": {
    "connectycube": "bin/connectycube.js"
  },
  "dependencies": {
    "@xmpp/client": "^0.13.5",
    "eventemitter3": "^5.0.1",
    "form-data": "4.0.3",
    "node-fetch": "2.7.0",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.0.10",
    "@types/node-fetch": "^2.6.12",
    "@types/xmpp__client": "^0.13.3",
    "jasmine": "^5.8.0",
    "jasmine-browser-runner": "^3.0.0",
    "jasmine-core": "^5.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nyc": "^17.1.0",
    "open-cli": "^8.0.0",
    "rollup": "^4.44.1",
    "rollup-plugin-circular-dependencies": "^1.1.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.44.1"
  }
}
